{
    "name"             : "nephre/skypebot",
    "description"      : "Skype DBUS API interface",
    "version"          : "1.0.0",
    "bin"              : [
        "bin/activechats.php",
        "bin/send.php",
        "bin/suchar.php",
        "bin/suchar_char.php"
    ],
    "config"           : {
        "bin-dir": "bin"
    },
    "keywords"         : ["skype", "dbus", "nephre", "bot", "php"],
    "homepage"         : "https://github.com/nephre/skypebot",
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "type"             : "library",
    "time"             : "2014-08-13",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Daniel Jeznach",
            "email": "djeznach@gmail.com",
            "role" : "Developer"
        }
    ],
    "autoload"         : {
        "psr-0": {
            "": "src/"
        }
    },
    "require"          : {
        "php"            : ">=5.4"
    },
    "suggest"          : {
        "fabpot/goutte"       : "For fetching jokes/entries from wykop.pl, using their API",
        "phpdoc/phpDocumentor": "For pretty API documentation"
    }
}
